  },
  "homepage": "https://github.com/guaca/new-blog#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "clean-css": "^5.3.0",
    "eleventy-plugin-youtube-embed": "^1.7.0",
    "luxon": "^3.0.1",
